{
  "name": "service-manti",
  "version": "1.0.0",
  "description": "module to work whis engine manticore search",
  "main": "index.js",
  "scripts": {
    "test": "jest --no-cache --runInBand --forceExit"
  },
  "keywords": [
    "manticore",
    "api"
  ],
  "author": "mr_sobol",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.21",
    "manticoresearch": "^3.1.0"
  },
  "devDependencies": {
    "supertest": "^6.1.6",
    "jest": "^27.5.1",
    "jest-cli": "^28.1.1",
    "jest-html-reporters": "^2.1.6",
    "jest-openapi": "^0.14.2",
    "jest-stare": "^2.4.1"
  },
  "jest": {
    "testMatch": [
      "**/?(*.)+(test).js"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ],
    "coverageDirectory": "./docs/.vuepress/public/coverage",
    "reporters": [
      [
        "./node_modules/jest-html-reporters",
        {
          "publicPath": "./docs/.vuepress/public/testReport",
          "filename": "index.html",
          "pageTitle": "@molfar/manticore-bridge"
        }
      ]
    ]
  }
}
